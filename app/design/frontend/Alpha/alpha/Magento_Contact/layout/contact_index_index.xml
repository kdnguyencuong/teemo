<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<head>
		<title>Contact Us - Commercial Kitchen &amp;amp; Catering Equipment in Sydney</title>
		<meta name="description" content="If you want more information on any of our catering equipment or supplies, then don't hesitate to contact us. Use our online form or call us on 1300 362 596."/>
		<meta name="keywords" content=" "/> 
	</head>
	<body>        
		<referenceBlock name="breadcrumbs">
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">Home</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Home</item>
                    <item name="label" xsi:type="string">Home</item>
                    <item name="link" xsi:type="string">/</item>
                </argument>
            </action>
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">Contact US</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Contact US</item>
                    <item name="label" xsi:type="string">Contact US</item>
                </argument>
            </action>
        </referenceBlock>
		 <referenceContainer name="content">
            <block class="Magento\Cms\Block\Block" name="contact.map" before="contactForm" >
				<arguments>
					<argument name="block_id" xsi:type="string">map-contact-page</argument>
				</arguments>
			</block>
			<block class="Magento\Cms\Block\Block" name="contact.right" after="-" >
				<arguments>
					<argument name="block_id" xsi:type="string">contact-us-right</argument>
				</arguments>
			</block>
        </referenceContainer>
		<referenceBlock name="page.main.title" remove="true"/>
    </body>
</page>
