<?xml version="1.0"?><!--
/**
 * Cart2Quote
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="quotation_quote_item_renderers"/>
	<head>
        <title>Faqs</title>
    </head>
    <body>
        <referenceBlock name="breadcrumbs">
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">Home</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Home</item>
                    <item name="label" xsi:type="string">Home</item>
                    <item name="link" xsi:type="string">/</item>
                </argument>
            </action>
            <action method="addCrumb">
                <argument name="crumbName" xsi:type="string">Quote List</argument>
                <argument name="crumbInfo" xsi:type="array">
                    <item name="title" xsi:type="string">Quote List</item>
                    <item name="label" xsi:type="string">Quote List</item>
                </argument>
            </action>
        </referenceBlock>
		<referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Request for Quote</argument>
            </action>
        </referenceBlock>
		<referenceBlock name="quotation.quote.customer.note" remove="true"/>
		
    </body>
</page>
